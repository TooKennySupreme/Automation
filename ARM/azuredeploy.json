{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
    },
    "variables": {
    },
    "resources": [
      {
        "name": "vNetSubnet",
        "type": "Microsoft.Resources/deployments",
        "apiVersion": "2018-05-01",
        "properties": {
          "mode": "Incremental",
          "templateLink": {
            "uri": "https://raw.githubusercontent.com/bigtob/main/master/ARM/Network/1vnet1subnet.json"
          },
          "parametersLink": {
            "uri": "https://raw.githubusercontent.com/bigtob/main/master/ARM/Network/1vnet1subnet.parameters.json"
          }
        }
      },
      {
        "name": "storageAccount",
        "type": "Microsoft.Resources/deployments",
        "apiVersion": "2018-05-01",
        "properties": {
          "mode": "Incremental",
          "templateLink": {
            "uri": "https://raw.githubusercontent.com/bigtob/main/master/ARM/StorageAccount/storageaccount.json"
          },
          "parameters": {
            "storageAccountType": {
              "value": "Standard_LRS"
            },
            "subnetId": {
              "value": "[reference('vNetSubnet').outputs.subnetId.value]"
            }
          }
        },
        "dependsOn": [
          "vNetSubnet"
        ]
      },
      {
        "name": "iLB",
        "type": "Microsoft.Resources/deployments",
        "apiVersion": "2018-05-01",
        "properties": {
          "mode": "Incremental",
          "templateLink": {
            "uri": "https://raw.githubusercontent.com/bigtob/main/master/ARM/iLB/iLB.json"
          },
          "parameters": {
            "loadBalancerName": {
              "value": "myiLB" 
            },
            "virtualNetworkName": {
              "value": "[reference('vNetSubnet').outputs.vnetId.value]"
            },
            "subnetName": {
              "value": "[reference('vNetSubnet').outputs.subnetId.value]"
            }
          }
        },
        "dependsOn": [
          "vNetSubnet"
        ]
      }  
    ],
    "outputs": {
    }
}