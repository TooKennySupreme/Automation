[grafana]
username = "{{ grafana_admin_user }}"
host = "localhost:3000"
protocol = "http"
password = "{{ grafana_admin_pass }}"
height = "280"
width = "640"
timerange = "3h"
enableLink = "no"
defaultdashboard = "base-metrics"
datasource = "graphite"
defaultdashboardstore = "db"
accessmode = "proxy"
timeout = "5"
directrefresh = "no"
